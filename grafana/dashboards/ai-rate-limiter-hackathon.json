{
  "id": null,
  "title": "ðŸ¤– AI Rate Limiter - Hackathon Demo",
  "tags": ["ai", "rate-limiting", "hackathon", "ollama"],
  "timezone": "browser",
  "refresh": "500ms",
  "time": {
    "from": "now-2m",
    "to": "now"
  },
    "panels": [
      {
        "id": 1,
        "title": "ðŸš€ AI vs Static Performance",
        "description": "Real-time request success rates showing AI's adaptive advantage over static rate limiting",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(rl_requests_total[1m])",
            "legendFormat": "{{tenant}} {{outcome}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {"displayMode": "gradient"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 50},
                {"color": "green", "value": 100}
              ]
            },
            "unit": "reqps"
          },
          "overrides": [
            {
              "matcher": {"id": "byRegexp", "options": ".*ent.*"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}},
                {"id": "displayName", "value": "ðŸ† Enterprise"}
              ]
            },
            {
              "matcher": {"id": "byRegexp", "options": ".*pro.*"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "blue"}},
                {"id": "displayName", "value": "ðŸ’¼ Professional"}
              ]
            },
            {
              "matcher": {"id": "byRegexp", "options": ".*free.*"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "orange"}},
                {"id": "displayName", "value": "ðŸ†“ Free"}
              ]
            }
          ]
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "ðŸ’° Revenue Protection (Real-time)",
        "description": "Business impact: Revenue protected vs lost during traffic surges. AI prevents revenue loss by smart scaling",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rl_revenue_protected_total",
            "legendFormat": "{{tenant}} Protected"
          },
          {
            "expr": "rl_revenue_lost_total",
            "legendFormat": "{{tenant}} Lost"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "currencyUSD"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "ðŸ§  AI Engine Health",
        "description": "AI decision confidence and engine status. High confidence (>85%) means reliable autonomous decisions",
        "type": "gauge",
        "targets": [
          {
            "expr": "avg(rl_ai_confidence_score)",
            "legendFormat": "AI Confidence"
          },
          {
            "expr": "rl_ai_engine_active",
            "legendFormat": "Engine Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.7},
                {"color": "green", "value": 0.85}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "ðŸŒŠ Surge Prediction",
        "description": "AI's predictive capability: Forecasts traffic surges before they happen (0-100% surge probability)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rl_surge_prediction",
            "legendFormat": "{{tenant}}/{{endpoint}} Surge %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "unit": "percentunit"
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8}
      },
      {
        "id": 5,
        "title": "ðŸ† Governance Queue (Auto-Approved)",
        "description": "Pending AI decisions requiring approval. Auto-approved after 0.5s for demo flow. Shows governance for large changes (>2x rate limit)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rl_governance_queue_size",
            "legendFormat": "Queue Size"
          },
          {
            "expr": "rate(rl_ai_decisions_total{applied=\"true\"}[30s]) * 60",
            "legendFormat": "Auto-Approvals/min"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 3}
              ]
            },
            "custom": {
              "displayMode": "gradient"
            }
          }
        },
        "options": {
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "displayMode": "gradient"
        },
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8}
      },
      {
        "id": 6,
        "title": "ðŸŽ¯ Customer Satisfaction",
        "description": "Customer experience score by tier. AI maintains high satisfaction while protecting system resources",
        "type": "bargauge",
        "targets": [
          {
            "expr": "rl_customer_satisfaction",
            "legendFormat": "{{tenant}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "color": {"mode": "continuous-GrYlRd"},
            "unit": "percentunit"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 7,
        "title": "ðŸ“Š AI Adaptive RPS Limits",
        "description": "Shows how AI dynamically adjusts rate limits (15-60 RPS) based on real-time traffic patterns vs static limits",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rl_policy_rps",
            "legendFormat": "AI {{tenant}} Dynamic"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"},
            "custom": {
              "thresholdsStyle": {
                "mode": "line"
              }
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 3},
                {"color": "yellow", "value": 8},
                {"color": "green", "value": 15}
              ]
            },
            "min": 0,
            "max": 80
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "AI ent Dynamic"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "AI pro Dynamic"},  
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "blue"}}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "AI free Dynamic"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "orange"}}
              ]
            }
          ]
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      }

  ],
  "schemaVersion": 30,
  "version": 1,
  "editable": true
}
