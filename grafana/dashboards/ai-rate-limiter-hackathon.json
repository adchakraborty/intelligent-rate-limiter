{
  "id": null,
  "title": "ü§ñ AI Rate Limiter - Hackathon Demo",
  "tags": ["ai", "rate-limiting", "hackathon", "ollama"],
  "timezone": "browser",
  "refresh": "1s",
  "time": {
    "from": "now-5m",
    "to": "now"
  },
    "panels": [
      {
        "id": 1,
        "title": "üöÄ AI vs Static Performance",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(rl_requests_total[1m])",
            "legendFormat": "{{tenant}} {{outcome}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {"displayMode": "gradient"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 50},
                {"color": "green", "value": 100}
              ]
            },
            "unit": "reqps"
          },
          "overrides": [
            {
              "matcher": {"id": "byRegexp", "options": ".*ent.*"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}},
                {"id": "displayName", "value": "üèÜ Enterprise"}
              ]
            },
            {
              "matcher": {"id": "byRegexp", "options": ".*pro.*"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "blue"}},
                {"id": "displayName", "value": "üíº Professional"}
              ]
            },
            {
              "matcher": {"id": "byRegexp", "options": ".*free.*"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "orange"}},
                {"id": "displayName", "value": "üÜì Free"}
              ]
            }
          ]
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "üí∞ Revenue Protection (Real-time)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rl_revenue_protected_total",
            "legendFormat": "{{tenant}} Protected"
          },
          {
            "expr": "rl_revenue_lost_total",
            "legendFormat": "{{tenant}} Lost"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "currencyUSD"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "üß† AI Engine Health",
        "type": "gauge",
        "targets": [
          {
            "expr": "avg(rl_ai_confidence_score)",
            "legendFormat": "AI Confidence"
          },
          {
            "expr": "rl_ai_engine_active",
            "legendFormat": "Engine Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.7},
                {"color": "green", "value": 0.85}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "üåä Surge Prediction",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rl_surge_prediction",
            "legendFormat": "{{tenant}}/{{endpoint}} Surge %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "unit": "percentunit"
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8}
      },
      {
        "id": 5,
        "title": "üèÜ Enterprise Priority Queue",
        "type": "stat",
        "targets": [
          {
            "expr": "rl_governance_queue_size",
            "legendFormat": "Pending Decisions"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 3}
              ]
            },
            "custom": {
              "displayMode": "gradient"
            }
          }
        },
        "options": {
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "displayMode": "gradient"
        },
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8}
      },
      {
        "id": 6,
        "title": "üéØ Customer Satisfaction",
        "type": "bargauge",
        "targets": [
          {
            "expr": "rl_customer_satisfaction",
            "legendFormat": "{{tenant}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "color": {"mode": "continuous-GrYlRd"},
            "unit": "percentunit"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 7,
        "title": "üìä AI Adaptive RPS Limits",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rl_policy_rps",
            "legendFormat": "AI {{tenant}} Dynamic"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"},
            "custom": {
              "thresholdsStyle": {
                "mode": "line"
              }
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 3},
                {"color": "yellow", "value": 8},
                {"color": "green", "value": 15}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "AI ent Dynamic"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "AI pro Dynamic"},  
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "blue"}}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "AI free Dynamic"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "orange"}}
              ]
            }
          ]
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 8,
        "title": "üî• AI Decision Stream",
        "type": "table",
        "targets": [
          {
            "expr": "increase(rl_ai_decisions_total[1m])",
            "legendFormat": "",
            "format": "table"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "tenant": "Customer Tier",
                "action": "AI Action",
                "applied": "Applied",
                "confidence_level": "Confidence"
              }
            }
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24}
      },
      {
        "id": 9,
        "title": "üèÜ Enterprise Scaling Advantage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rl_policy_rps{tenant=\"ent\"}",
            "legendFormat": "üèÜ Enterprise Adaptive Limit"
          },
          {
            "expr": "rl_policy_rps{tenant=\"pro\"}",
            "legendFormat": "üíº Professional Limit"
          },
          {
            "expr": "rl_policy_rps{tenant=\"free\"}",
            "legendFormat": "üÜì Free Tier Limit"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"},
            "custom": {
              "lineWidth": 3,
              "fillOpacity": 10,
              "gradientMode": "scheme"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "transparent", "value": null},
                {"color": "green", "value": 15}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "üèÜ Enterprise Adaptive Limit"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}},
                {"id": "custom.lineWidth", "value": 4},
                {"id": "custom.fillOpacity", "value": 20}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "üíº Professional Limit"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "blue"}},
                {"id": "custom.lineWidth", "value": 2}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "üÜì Free Tier Limit"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "orange"}},
                {"id": "custom.lineWidth", "value": 1}
              ]
            }
          ]
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "bottom",
            "calcs": ["lastNotNull", "max"]
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32}
      }
  ],
  "schemaVersion": 30,
  "version": 1,
  "editable": true
}
